<!--  This application utilizes viewmodel first and is supported with DataTemplates. If you are to use DataTemplates ensure you load the ResourceDictionary into the resources of the App.xaml  -->
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dashboards="clr-namespace:WpfDashboardControl.Dashboards">
    <DataTemplate DataType="{x:Type dashboards:DashboardsViewModel}">
        <dashboards:DashboardsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type dashboards:DashboardSettingsPromptViewModel}">
        <dashboards:DashboardSettingsPromptView />
    </DataTemplate>
</ResourceDictionary>